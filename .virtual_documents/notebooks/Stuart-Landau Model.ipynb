    MI_23 = gcmi_nd_cc(R2, stim, traxis=0)
    MI_123 = gccmi_nd_ccc(R1, R2, stim, traxis=0)
    Red = numpy.minimum(MI_13, MI_23)
    return MI_123 - MI_13 - MI_23 + Red


